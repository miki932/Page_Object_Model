{
  "mode": "testops",
  "fallback": "report",
  "report": {
    "driver": "local",
    "connection": {
      "local": {
        "path": "./build/qase-report",
        "format": "json"
      }
    }
  },
  "testops": {
    "project": "<INSERT-HERE-YOUR-PROJECT-ID>",
    "api": {
      "token": "<INSERT-HERE-YOUR-PROJECT-TOKEN>"
    },
    "run": {
      "complete": true
    },
    "defect": false
  },
  "framework": {
    "pytest": {
      "capture": {
        "logs": true,
        "http": true
      }
    }
  }
}